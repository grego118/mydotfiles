[user]
    name = Austin Gregory
    email = austinggregory@gmail.com
[core]
    autocrlf = input
    editor = /usr/local/bin/nvim
[merge]
    tool = kdiff3
[diff]
    guitool = kdiff3
[alias]
    aa = add --all
    ci = commit
    ll = log --graph --oneline --decorate --all
    st = status -sb
    co = checkout
    br = branch
    rb = rebase
    rs = reset
    fo = fetch origin
[push]
    default = simple
[pull]
    rebase = true
[url "git@github.com:"]
    insteadOf = https://github.com/
